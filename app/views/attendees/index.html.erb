<h1><%= @event.title %></h1>
<h2>Attendees</h2>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Source</th>
    </tr>
  </thead>
  <% @attendees.each do |attendee| %>
    <tr>
      <td><%= attendee["first_name"] %> <%= attendee["last_name"] %></td>
      <td><%= attendee["source"] %></td>
    </tr>
  <% end %>
</table>